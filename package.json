{
  "name": "fastify-language-parser",
  "version": "2.1.0",
  "description": "Language parser plugin for fastify",
  "main": "plugin.js",
  "types": "pludin.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "npm run lint:standard && npm run lint:typescript",
    "lint:standard": "standard",
    "lint:fix": "standard --fix",
    "lint:typescript": "standard --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin test/types/*.ts",
    "test": " standard --fix && tap test.js --cov --no-browser",
    "coverage": "standard --fix && tap test.js --cov --no-browser --coverage-report html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lependu/fastify-language-parser.git"
  },
  "keywords": [
    "nodejs",
    "fastify",
    "i18n"
  ],
  "author": "Akos KovÃ¡cs <lependu@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lependu/fastify-language-parser/issues"
  },
  "homepage": "https://github.com/lependu/fastify-language-parser#readme",
  "devDependencies": {
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "fastify": "^2.4.1",
    "standard": "^14.0.0",
    "tap": "^12.7.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "fastify-plugin": "^1.6.0"
  },
  "greenkeeper": {
    "ignore": [
      "tap"
    ]
  }
}
